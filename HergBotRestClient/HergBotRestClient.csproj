<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>HergBot.RestClient</PackageId>
    <Authors>Justin</Authors>
    <Company>HergBot</Company>
    <Product>HergBot.RestClient</Product>
    <Description>A library for communicating with RESTful API's.</Description>
    <RepositoryUrl>https://github.com/HergBot/rest-client-csharp</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageReleaseNotes>The initial release of the package.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Castle.Core" Version="4.4.0" />
    <PackageReference Include="HergBot.Utilities" Version="1.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
  </ItemGroup>

</Project>
